{
  "name": "dynamom",
  "version": "0.0.10",
  "description": "orm for dynamoDB",
  "keywords": [
    "dynamodb",
    "database",
    "orm",
    "typescript",
    "dynamo1"
  ],
  "author": "Moka <aud1220a@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/moka-a/dynamom.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepack": "npm run test && npm run build",
    "build": "rimraf ./lib && tsc --emitDeclarationOnly && babel src --out-dir lib --extensions \".ts\" --source-maps inline",
    "test": "npm run build && npm run test:lint && npm run test:unit",
    "test:lint": "eslint src/**/*",
    "test:unit": "jest test"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "[^/]*\\.test.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2",
    "core-js": "^3.0.0",
    "kuuid": "^0.5.0",
    "relater": "^0.1.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-typescript": "^7.1.0",
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.0",
    "@types/node": "^11.9.4",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^1.4.0",
    "eslint": "^5.14.1",
    "faker": "^4.1.0",
    "jest": "^24.1.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.0.0",
    "typescript": "^3.2.2"
  }
}
